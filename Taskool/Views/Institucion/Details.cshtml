@model Taskool.Models.Institucion

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

<fieldset>
    <legend>Institucion</legend>

    <div class="display-label">
         NIT
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.nit)
    </div>

    <div class="display-label">
        Razon Social
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.razonSocial)
